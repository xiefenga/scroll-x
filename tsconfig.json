{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "module": "ESNext",
    "rootDir": "./",
    "moduleResolution": "node",
    "baseUrl": "./",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./",
    "removeComments": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@/*":[
        "src/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ],
}